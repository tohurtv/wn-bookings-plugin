{
    "name": "tohur/bookings-plugin",
    "type": "winter-plugin",
    "extra": {
        "installer-name": "bookings"
    },
    "description": "Bookings plugin for WinterCMS",
    "homepage": "https://github.com/tohurtv/wn-bookings-plugin",
    "keywords": ["winter", "plugin", "bookings"],
    "license": "MIT",
    "authors": [
        {
            "name": "Joshua Webb",
